<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.cheryl.petit">

    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.VIBRATE" />
    <uses-permission android:name="android.permission.WAKE_LOCK" />

    <application
        android:allowBackup="true"
        android:hardwareAccelerated="true"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".ActivityDetails"/>
        <activity android:name=".Parkpage" />
        <activity android:name=".Hotelpage" />
        <activity android:name=".user" />
        <activity android:name=".record" />
        <activity android:name=".search" />
        <activity android:name=".Helpmodel" />
        <activity android:name=".opinion">
            <intent-filter>
                <action android:name="android.intent.action.SEND" />

                <category android:name="android.intent.category.DEFAULT" />

                <data android:mimeType="message/rfc822" />
            </intent-filter>
        </activity>
        <activity android:name=".addsupp" />
        <activity android:name=".adddoc" />
        <activity android:name=".addcal" />
        <activity android:name=".petadd" />
        <activity android:name=".restaurant" />
        <activity android:name=".salon" />
        <activity android:name=".hospital" />
        <activity android:name=".hotel" />
        <activity android:name=".place" />
        <activity android:name=".park" />
        <activity android:name=".supp" />
        <activity android:name=".doc" />
        <activity android:name=".cal" />
        <activity android:name=".favorite" />
        <activity android:name=".pet" />
        <activity android:name=".homepage" />
        <activity android:name=".welcome">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <receiver android:name=".Receiver" />
    </application>

</manifest>